.pricing-model {
  background: var(--bg-tertiary);
  padding: 6rem 0;
  position: relative;
}

.pricing-model::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: 
    radial-gradient(circle at 20% 20%, rgba(6, 182, 212, 0.08) 0%, transparent 50%),
    radial-gradient(circle at 80% 80%, rgba(37, 99, 235, 0.06) 0%, transparent 50%);
  pointer-events: none;
}

.pricing-model h2 {
  text-align: center;
  font-size: clamp(2.5rem, 5vw, 3.5rem);
  font-weight: 700;
  background: var(--gradient-primary);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  margin-bottom: 2rem;
  position: relative;
  z-index: 1;
  padding-bottom: 1rem;
}

.pricing-model h2::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 60px;
  height: 3px;
  background: var(--gradient-primary);
  border-radius: 2px;
  transform: translateX(-50%);
}

.pricing-intro {
  text-align: center;
  max-width: 800px;
  margin: 0 auto 3rem auto;
  font-size: clamp(1.1rem, 2vw, 1.25rem);
  color: var(--text-secondary);
  line-height: 1.7;
  position: relative;
  z-index: 1;
}

.pricing-process {
  position: relative;
  z-index: 1;
}

.process-title {
  text-align: center;
  margin-bottom: 3rem;
}

.process-title h3 {
  color: var(--text-primary);
  font-size: 1.8rem;
  font-weight: 600;
  margin-bottom: 1rem;
}

.process-title p {
  color: var(--text-secondary);
  font-size: 1.1rem;
  max-width: 600px;
  margin: 0 auto;
}

.pricing-steps {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 1.5rem;
  max-width: 1200px;
  margin: 0 auto 4rem auto;
  position: relative;
  z-index: 1;
}

.pricing-step {
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(20px);
  border-radius: 24px;
  border: 2px solid var(--border-color);
  padding: 2.5rem 2rem;
  width: 300px;
  min-height: 400px;
  display: flex;
  flex-direction: column;
  transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  position: relative;
  overflow: hidden;
}

.pricing-step::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: var(--gradient-primary);
  opacity: 0;
  transition: opacity 0.3s ease;
  z-index: -1;
}

.pricing-step:hover {
  transform: translateY(-10px);
  border-color: var(--primary-light);
  background: rgba(255, 255, 255, 0.06);
  box-shadow: 0 20px 50px var(--shadow-color), 0 10px 30px var(--shadow-accent);
}

.pricing-step:hover::before {
  opacity: 0.05;
}

.step-1 {
  border-color: #3b82f6;
}

.step-1:hover {
  border-color: #60a5fa;
  box-shadow: 0 20px 50px rgba(59, 130, 246, 0.3), 0 10px 30px rgba(59, 130, 246, 0.2);
}

.step-2 {
  border-color: #1e40af;
}

.step-2:hover {
  border-color: #3b82f6;
  box-shadow: 0 20px 50px rgba(30, 64, 175, 0.3), 0 10px 30px rgba(30, 64, 175, 0.2);
}

.step-3 {
  border-color: #84cc16;
}

.step-3:hover {
  border-color: #a3e635;
  box-shadow: 0 20px 50px rgba(132, 204, 22, 0.3), 0 10px 30px rgba(132, 204, 22, 0.2);
}

.step-header {
  text-align: center;
  margin-bottom: 2rem;
}

.step-number {
  width: 70px;
  height: 70px;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 1.8rem;
  font-weight: 700;
  margin: 0 auto 1rem auto;
  box-shadow: 0 8px 25px rgba(0, 0, 0, 0.3);
  transition: all 0.3s ease;
}

.step-1 .step-number {
  background: linear-gradient(135deg, #3b82f6 0%, #1d4ed8 100%);
}

.step-2 .step-number {
  background: linear-gradient(135deg, #1e40af 0%, #1e3a8a 100%);
}

.step-3 .step-number {
  background: linear-gradient(135deg, #84cc16 0%, #65a30d 100%);
}

.step-header h4 {
  color: var(--text-primary);
  font-size: 1.4rem;
  font-weight: 700;
  margin: 0;
}

.step-content {
  display: flex;
  flex-direction: column;
  flex: 1;
  text-align: center;
}

.step-features {
  list-style: none;
  padding: 0;
  margin: 0 0 2rem 0;
  flex: 1;
}

.step-features li {
  color: var(--text-secondary);
  font-size: 1rem;
  font-weight: 500;
  line-height: 1.6;
  margin-bottom: 0.75rem;
  padding: 0.5rem 0;
  position: relative;
}

.step-features li::before {
  content: 'âœ“';
  color: var(--primary-light);
  font-weight: 700;
  margin-right: 0.75rem;
  font-size: 1.1rem;
}

.step-duration {
  color: var(--text-primary);
  font-size: 1rem;
  font-weight: 600;
  margin-bottom: 1rem;
  padding: 0.75rem 1rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 12px;
  border: 1px solid rgba(255, 255, 255, 0.1);
}

.step-price {
  color: var(--primary-light);
  font-size: 1.1rem;
  font-weight: 700;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  padding: 1rem;
  background: rgba(92, 159, 255, 0.1);
  border-radius: 12px;
  border: 1px solid rgba(92, 159, 255, 0.2);
}

.step-arrow {
  color: var(--primary-light);
  opacity: 0.8;
  transition: all 0.3s ease;
  flex-shrink: 0;
}

.step-arrow svg {
  width: 40px;
  height: 40px;
  filter: drop-shadow(0 2px 4px rgba(0, 0, 0, 0.2));
}

.step-arrow:hover {
  opacity: 1;
  transform: translateX(5px);
}

.pricing-explanation {
  margin: 4rem auto 3rem auto;
  max-width: 900px;
  display: flex;
  flex-direction: column;
  gap: 2rem;
  position: relative;
  z-index: 1;
}

.pricing-point {
  display: flex;
  align-items: flex-start;
  gap: 1.5rem;
  background: rgba(255, 255, 255, 0.02);
  backdrop-filter: blur(15px);
  padding: 2rem;
  border-radius: 16px;
  border: 1px solid var(--border-color);
  transition: all 0.3s ease;
}

.pricing-point:hover {
  transform: translateY(-3px);
  border-color: var(--primary-light);
  background: rgba(255, 255, 255, 0.04);
}

.pricing-icon {
  flex-shrink: 0;
  width: 48px;
  height: 48px;
  background: var(--gradient-primary);
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
}

.pricing-icon svg {
  width: 24px;
  height: 24px;
}

.pricing-text h6 {
  color: var(--primary-light);
  font-size: 1.2rem;
  font-weight: 600;
  margin-bottom: 0.75rem;
}

.pricing-text p {
  color: var(--text-secondary);
  line-height: 1.6;
  margin: 0;
}

.pricing-cta {
  text-align: center;
  max-width: 700px;
  margin: 0 auto;
  position: relative;
  z-index: 1;
}

.pricing-cta p {
  color: var(--text-secondary);
  font-size: 1.1rem;
  line-height: 1.6;
  margin-bottom: 2rem;
}

.pricing-cta .btn {
  font-size: 1.1rem;
  padding: 1.2rem 2.5rem;
}

/* Journey Section */
.journey {
  background: var(--bg-primary);
  padding: 8rem 0;
  position: relative;
  overflow: hidden;
}

.journey::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: radial-gradient(circle at 30% 50%, rgba(168, 85, 247, 0.1) 0%, transparent 50%),
              radial-gradient(circle at 70% 80%, rgba(59, 130, 246, 0.08) 0%, transparent 50%);
  pointer-events: none;
}

.journey h2 {
  color: var(--text-primary);
  font-size: 3rem;
  font-weight: 700;
  text-align: center;
  margin-bottom: 1.5rem;
  position: relative;
  z-index: 1;
}

.journey-intro {
  color: var(--text-secondary);
  font-size: 1.2rem;
  text-align: center;
  max-width: 800px;
  margin: 0 auto 4rem auto;
  line-height: 1.6;
  position: relative;
  z-index: 1;
}

.journey-timeline {
  position: relative;
  max-width: 800px;
  margin: 0 auto 4rem auto;
  padding: 2rem 0;
  z-index: 1;
}

.timeline-line {
  position: absolute;
  left: 50%;
  top: 0;
  bottom: 0;
  width: 3px;
  background: linear-gradient(
    to bottom,
    transparent 0%,
    var(--primary-light) 10%,
    var(--primary-light) 90%,
    transparent 100%
  );
  transform: translateX(-50%);
  z-index: 0;
}

.timeline-line::before {
  content: '';
  position: absolute;
  left: 50%;
  top: 0;
  bottom: 0;
  width: 1px;
  background: var(--gradient-primary);
  transform: translateX(-50%);
  opacity: 0.6;
  animation: timelineGlow 3s ease-in-out infinite alternate;
  z-index: -1;
}

@keyframes timelineGlow {
  0% { box-shadow: 0 0 5px rgba(168, 85, 247, 0.3); }
  100% { box-shadow: 0 0 20px rgba(168, 85, 247, 0.6), 0 0 30px rgba(59, 130, 246, 0.3); }
}

.timeline-item {
  position: relative;
  margin-bottom: 4rem;
  display: flex;
  align-items: flex-start;
  opacity: 0;
  transform: translateY(30px);
  transition: all 0.6s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  z-index: 2;
}

.timeline-item.visible {
  opacity: 1;
  transform: translateY(0);
}

.timeline-item:nth-child(even) {
  flex-direction: row-reverse;
}

.timeline-item:nth-child(even) .timeline-content {
  text-align: right;
  margin-right: 0;
  margin-left: 150px;
}

.timeline-item:nth-child(odd) .timeline-content {
  text-align: left;
  margin-left: 0;
  margin-right: 150px;
}

.timeline-dot {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  z-index: 3;
  width: 60px;
  height: 60px;
  background: var(--gradient-primary);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 
    0 0 0 4px var(--bg-primary),
    0 0 0 8px rgba(168, 85, 247, 0.2),
    0 8px 25px rgba(168, 85, 247, 0.3);
  transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  flex-shrink: 0;
}

.timeline-dot:hover {
  transform: translateX(-50%) scale(1.1);
  box-shadow: 
    0 0 0 4px var(--bg-primary),
    0 0 0 12px rgba(168, 85, 247, 0.3),
    0 12px 35px rgba(168, 85, 247, 0.4),
    0 0 40px rgba(59, 130, 246, 0.3);
}

.dot-inner {
  font-weight: 700;
  font-size: 1.2rem;
  color: white;
  text-shadow: 0 2px 4px rgba(0, 0, 0, 0.3);
}

.timeline-content {
  flex: 1;
  max-width: 350px;
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(20px);
  border-radius: 20px;
  border: 2px solid var(--border-color);
  padding: 2rem;
  position: relative;
  overflow: hidden;
  transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  z-index: 1;
}

.timeline-content::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: var(--gradient-primary);
  opacity: 0;
  transition: opacity 0.3s ease;
  z-index: -1;
}

.timeline-content:hover {
  transform: translateY(-5px);
  border-color: var(--primary-light);
  background: rgba(255, 255, 255, 0.06);
  box-shadow: 0 15px 40px var(--shadow-color), 0 8px 25px var(--shadow-accent);
}

.timeline-content:hover::before {
  opacity: 0.05;
}

/* Cross-hover effects between content and dots */
.timeline-item:hover .timeline-dot {
  transform: translateX(-50%) scale(1.1);
  box-shadow: 
    0 0 0 4px var(--bg-primary),
    0 0 0 12px rgba(168, 85, 247, 0.3),
    0 12px 35px rgba(168, 85, 247, 0.4),
    0 0 40px rgba(59, 130, 246, 0.3);
}

.timeline-item:hover .timeline-content {
  transform: translateY(-5px);
  border-color: var(--primary-light);
  background: rgba(255, 255, 255, 0.06);
  box-shadow: 0 15px 40px var(--shadow-color), 0 8px 25px var(--shadow-accent);
}

.timeline-item:hover .timeline-content::before {
  opacity: 0.05;
}

.timeline-content h4 {
  color: var(--text-primary);
  font-size: 1.4rem;
  font-weight: 600;
  margin-bottom: 1rem;
  line-height: 1.3;
}

.timeline-content p {
  color: var(--text-secondary);
  font-size: 1rem;
  line-height: 1.6;
  margin: 0;
}

.journey-conclusion {
  max-width: 800px;
  margin: 0 auto;
  position: relative;
  z-index: 1;
}

.conclusion-card {
  background: rgba(255, 255, 255, 0.05);
  backdrop-filter: blur(20px);
  border-radius: 24px;
  border: 2px solid var(--border-color);
  padding: 3rem;
  text-align: center;
  transition: all 0.3s ease;
}

.conclusion-card:hover {
  border-color: var(--primary-light);
  background: rgba(255, 255, 255, 0.08);
  box-shadow: 0 10px 30px var(--shadow-color);
}

.conclusion-card h5 {
  color: var(--primary-light);
  font-size: 1.5rem;
  font-weight: 600;
  margin-bottom: 1rem;
}

.conclusion-card p {
  color: var(--text-secondary);
  font-size: 1.1rem;
  line-height: 1.6;
  margin: 0;
}

/* Agents Hero Section */
.agents-hero {
  background: var(--gradient-hero);
  padding: 10rem 0 8rem; 
  text-align: center; 
  position: relative;
  margin-top: 0;
  overflow: hidden;
  min-height: 100vh;
  display: flex;
  align-items: center;
}

.agents-hero::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: 
    radial-gradient(circle at 20% 80%, rgba(37, 99, 235, 0.25) 0%, transparent 65%),
    radial-gradient(circle at 80% 20%, rgba(14, 165, 233, 0.2) 0%, transparent 65%),
    radial-gradient(circle at 40% 40%, rgba(6, 182, 212, 0.15) 0%, transparent 45%);
  pointer-events: none;
}

#agents-particle-canvas,
#security-particle-canvas,
#team-particle-canvas,
#contact-particle-canvas {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.9;
  pointer-events: none;
  mix-blend-mode: screen;
}

.agents-hero .container,
.security-hero .container,
.team-hero .container,
.contact-hero .container {
  position: relative; 
  z-index: 1;
  width: 100%;
}

.agents-hero h1 { 
  font-size: clamp(2.5rem, 5vw, 4rem);
  margin-bottom: 1.5rem; 
  color: #ffffff;
  font-weight: 700;
  line-height: 1.2;
}

.agents-hero p { 
  font-size: clamp(1.1rem, 2vw, 1.3rem);
  margin-bottom: 3rem; 
  color: #f1f5f9;
  max-width: 600px;
  margin-left: auto;
  margin-right: auto;
  line-height: 1.6;
  font-weight: 500;
}

.hero-stats {
  display: flex;
  justify-content: center;
  gap: 4rem;
  margin-top: 3rem;
}

.stat-item {
  text-align: center;
  position: relative;
}

.stat-number {
  font-size: clamp(2.5rem, 4vw, 3.5rem);
  font-weight: 700;
  color: white;
  line-height: 1;
  margin-bottom: 0.5rem;
  background: var(--gradient-primary);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.stat-label {
  font-size: 1rem;
  color: var(--text-secondary);
  font-weight: 500;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

/* Philosophy Section */
.philosophy {
  background: var(--bg-secondary);
  padding: 6rem 0;
  position: relative;
  overflow: hidden;
}

.philosophy::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: 
    radial-gradient(circle at 30% 20%, rgba(59, 130, 246, 0.08) 0%, transparent 50%),
    radial-gradient(circle at 70% 80%, rgba(37, 99, 235, 0.06) 0%, transparent 50%);
  pointer-events: none;
}

.philosophy h2 {
  margin-bottom: 3rem;
  font-size: clamp(2.5rem, 5vw, 3.5rem);
  font-weight: 700;
  background: var(--gradient-primary);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  text-align: center;
  letter-spacing: -0.02em;
  line-height: 1.1;
  position: relative;
  padding-bottom: 1rem;
  z-index: 1;
}

.philosophy h2::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 50%;
  width: 60px;
  height: 3px;
  background: var(--gradient-primary);
  border-radius: 2px;
  transform: translateX(-50%);
}

.philosophy-intro {
  color: var(--text-secondary);
  font-size: 1.2rem;
  text-align: center;
  max-width: 700px;
  margin: 0 auto 4rem auto;
  line-height: 1.6;
  position: relative;
  z-index: 1;
}

.philosophy-content {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(400px, 1fr));
  gap: 3rem;
  max-width: 1200px;
  margin: 0 auto 4rem auto;
  position: relative;
  z-index: 1;
}

.philosophy-card {
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(20px);
  border-radius: 24px;
  border: 2px solid var(--border-color);
  padding: 3rem;
  display: flex;
  align-items: flex-start;
  gap: 2rem;
  transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  position: relative;
  overflow: hidden;
}

.philosophy-card::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: var(--gradient-primary);
  opacity: 0;
  transition: opacity 0.3s ease;
  z-index: -1;
}

.philosophy-card:hover {
  transform: translateY(-8px);
  border-color: var(--primary-light);
  background: rgba(255, 255, 255, 0.06);
  box-shadow: 0 20px 50px var(--shadow-color), 0 10px 30px var(--shadow-accent);
}

.philosophy-card:hover::before {
  opacity: 0.05;
}

.philosophy-icon {
  width: 80px;
  height: 80px;
  background: var(--gradient-primary);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
  box-shadow: 0 8px 25px rgba(59, 130, 246, 0.3);
  transition: all 0.3s ease;
}

.philosophy-card:hover .philosophy-icon {
  transform: scale(1.1);
  box-shadow: 0 12px 35px rgba(59, 130, 246, 0.4);
}

.philosophy-svg {
  width: 40px;
  height: 40px;
  color: white;
}

.philosophy-text h3 {
  color: var(--text-primary);
  font-size: 1.5rem;
  font-weight: 600;
  margin-bottom: 1rem;
  line-height: 1.3;
}

.philosophy-text p {
  color: var(--text-secondary);
  font-size: 1rem;
  line-height: 1.6;
  margin: 0;
}

.philosophy-benefits {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));
  gap: 2rem;
  max-width: 1200px;
  margin: 0 auto;
  position: relative;
  z-index: 1;
}

.benefit-item {
  display: flex;
  align-items: flex-start;
  gap: 1.5rem;
  background: rgba(255, 255, 255, 0.03);
  backdrop-filter: blur(15px);
  border-radius: 20px;
  padding: 2rem;
  border: 2px solid var(--border-color);
  transition: all 0.4s cubic-bezier(0.25, 0.46, 0.45, 0.94);
  position: relative;
  overflow: hidden;
}

.benefit-item::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: var(--gradient-primary);
  opacity: 0;
  transition: opacity 0.3s ease;
  z-index: -1;
}

.benefit-item:hover {
  background: rgba(255, 255, 255, 0.06);
  border-color: var(--primary-light);
  transform: translateY(-5px);
  box-shadow: 0 15px 40px var(--shadow-color), 0 8px 25px var(--shadow-accent);
}

.benefit-item:hover::before {
  opacity: 0.05;
}

.benefit-icon {
  width: 50px;
  height: 50px;
  background: var(--gradient-primary);
  border-radius: 12px;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
  box-shadow: 0 6px 20px rgba(59, 130, 246, 0.3);
  transition: all 0.3s ease;
}

.benefit-item:hover .benefit-icon {
  transform: scale(1.1);
  box-shadow: 0 8px 25px rgba(59, 130, 246, 0.4);
}

.benefit-svg {
  width: 24px;
  height: 24px;
  color: white;
}

.benefit-text h4 {
  color: var(--text-primary);
  font-size: 1.1rem;
  font-weight: 600;
