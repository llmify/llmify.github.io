/* Flip Card Structure */
.flip-card {
  position: relative;
  width: 100%;
  height: 100%;
  text-align: center;
}

.flip-card-inner {
  position: relative;
  width: 100%;
  height: 100%;
  text-align: center;
  transition: transform 0.7s cubic-bezier(0.25, 0.46, 0.45, 0.94), box-shadow 0.7s ease;
  transform-style: preserve-3d;
  transform-origin: center center;
}

/* Add subtle hover effect to the entire team member */
.team-member {
  transition: transform 0.3s ease;
}

.team-member:hover {
  transform: translateY(-5px);
}

.flip-card-front, .flip-card-back {
  position: absolute;
  width: 100%;
  height: 100%;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  transform-style: preserve-3d;
  background: none !important;
  border: none !important;
  box-shadow: none !important;
  border-radius: 0 !important;
  overflow: visible !important;
}

/* Front card styling moved to photo and info containers */

.flip-card-back {
  background: none !important;
  backdrop-filter: none !important;
  transform: rotateY(180deg);
  display: block;
  padding: 0;
  border-radius: 0 !important;
  overflow: visible !important;
  box-shadow: none !important;
  border: none !important;
}

/* Front of Card */
.team-photo {
  width: 100%;
  height: 370px;
  overflow: hidden;
  position: relative;
  background: var(--bg-tertiary);
  border-radius: 24px 24px 0 0;
  box-shadow: 0 15px 35px var(--shadow-color), 0 8px 20px var(--shadow-accent);
}

.team-photo img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: center top;
  transition: all 0.3s ease;
  border-radius: 24px 24px 0 0;
}

.team-front-info {
  padding: 1.5rem;
