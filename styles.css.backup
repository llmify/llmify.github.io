    transform: translateX(-50%) scale(1.1);
  }

  .timeline-item:hover .timeline-dot {
    transform: translateX(-50%) scale(1.1);
    box-shadow: 
      0 0 0 3px var(--bg-primary),
      0 0 0 8px rgba(168, 85, 247, 0.3),
      0 8px 25px rgba(168, 85, 247, 0.4);
  }

  .timeline-item:hover .timeline-content {
    transform: translateY(-3px);
    border-color: var(--primary-light);
    background: rgba(255, 255, 255, 0.06);
    box-shadow: 0 10px 30px var(--shadow-color), 0 5px 20px var(--shadow-accent);
  }

  .dot-inner {
    font-size: 1.1rem;
  }

  .timeline-content {
    padding: 1.5rem;
    max-width: none;
  }

  .timeline-content h4 {
    font-size: 1.2rem;
  }

  .timeline-content p {
    font-size: 0.95rem;
  }

  .conclusion-card {
    padding: 2rem;
  }

  .conclusion-card h5 {
    font-size: 1.3rem;
  }

  .conclusion-card p {
    font-size: 1rem;
  }

  /* Philosophy Section Mobile */
  .philosophy {
    padding: 4rem 0;
  }

  .philosophy h2 {
    font-size: clamp(2rem, 6vw, 2.5rem);
  }

  .philosophy-intro {
    font-size: 1.1rem;
    margin-bottom: 3rem;
  }

  .philosophy-content {
    grid-template-columns: 1fr;
    gap: 2rem;
    margin-bottom: 3rem;
  }

  .philosophy-card {
    flex-direction: column;
    text-align: center;
    padding: 2.5rem 2rem;
    gap: 1.5rem;
  }

  .philosophy-icon {
    width: 70px;
    height: 70px;
    margin: 0 auto;
  }

  .philosophy-svg {
    width: 35px;
    height: 35px;
  }

  .philosophy-text h3 {
    font-size: 1.3rem;
  }

  .philosophy-text p {
    font-size: 0.95rem;
  }

  .philosophy-benefits {
    grid-template-columns: 1fr;
    gap: 1.5rem;
  }

  .benefit-item {
    flex-direction: column;
    text-align: center;
    padding: 2rem 1.5rem;
    gap: 1rem;
  }

  .benefit-icon {
    width: 45px;
    height: 45px;
    margin: 0 auto;
  }

  .benefit-svg {
    width: 22px;
    height: 22px;
  }

  .benefit-text h4 {
    font-size: 1rem;
  }

  .benefit-text p {
    font-size: 0.85rem;
  }

  /* Contact Information Section Mobile */
  .contact-information {
    padding: 4rem 0;
  }

  .contact-information h2 {
    font-size: 2.2rem;
  }

  .contact-info-intro {
    font-size: 1.1rem;
    margin-bottom: 3rem;
  }

  .contact-info-grid {
    grid-template-columns: 1fr;
    gap: 2rem;
    margin: 0 1rem;
  }

  .info-card {
    padding: 2.5rem 2rem;
  }

  .info-icon {
    width: 70px;
    height: 70px;
    margin-bottom: 1.5rem;
  }

  .info-svg {
    width: 35px;
    height: 35px;
  }

  .info-card h3 {
    font-size: 1.3rem;
    margin-bottom: 1.2rem;
  }

  .info-details p {
    font-size: 0.95rem;
  }

}
